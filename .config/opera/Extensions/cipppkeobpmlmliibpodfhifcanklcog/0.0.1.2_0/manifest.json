{
   "background": {
      "persistent": false,
      "scripts": [ "options_prefs.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon19.png",
         "38": "img/icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "ColorPicker Eyedropper"
   },
   "content_scripts": [ {
      "js": [ "Cr_min.js", "options_prefs.js", "colorpick.user.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "A color-picker tool that allows you to select color values from webpages.",
   "developer": {
      "name": "dreamcarved"
   },
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "64": "img/icon64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ColorPicker Eyedropper",
   "permissions": [ "storage", "tabs", "\u003Call_urls>", "alarms" ],
   "update_url": "https://extension-updates.opera.com/api/omaha/update/",
   "version": "0.0.1.2",
   "web_accessible_resources": [ "img/crosshair.png", "img/close.png" ]
}
